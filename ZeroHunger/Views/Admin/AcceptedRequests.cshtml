
@{
    ViewBag.Title = "AcceptedRequests";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h2>AcceptedRequests</h2>

<br />
<br />
<table class="table table-bordered">

    <tr>
        <th>ID</th>
        <th>Restaurant Name</th>
        <th>Food Name</th>
        <th>Food Quantity (KG)</th>
        <th>Max Preserve Time</th>
        <th>Employee Name</th>
        <th>Status</th>

    </tr>

    @foreach (var data in Model)
    {
        <tr>
            <td>@data.Id</td>
            <td>@data.Restaurant.Name</td>
            <td>@data.FoodName</td>
            <td>@data.FoodQuantity</td>
            <td>@data.MaxPreserveTime</td>
            <td>@data.Employee.Name</td>
            <td>@data.Status</td>
        </tr>
    }

</table>



